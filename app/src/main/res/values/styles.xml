<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <!--FallDetection-->
    <style name="AboutTitle">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/FadeRed</item>
        <item name="android:gravity">left</item>
    </style>
    <style name="AboutText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">left</item>
    </style>
    <style name="AlarmText">
        <item name="android:textSize">28.0sp</item>
        <item name="android:textColor">@color/FadeYellow</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="AlertTitleText">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/FadeRed</item>
        <item name="android:gravity">left</item>
        <item name="android:paddingLeft">60.0dip</item>
    </style>
    <style name="AlertText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">left</item>
        <item name="android:paddingLeft">60.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="CalibratingText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">160.0dip</item>
    </style>
    <style name="CancelAlertText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/FadeGreen</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="ContactText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center|left</item>
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:paddingRight">40.0dip</item>
        <item name="android:maxWidth">140.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ContactTextData">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center|left</item>
        <item name="android:paddingLeft">40.0dip</item>
        <item name="android:paddingRight">42.0dip</item>
        <item name="android:maxWidth">140.0dip</item>
        <item name="android:lines">2</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
    </style>
    <style name="CountdownText">
        <item name="android:textSize">28.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="DialogButton">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center|left</item>
        <item name="android:paddingLeft">50.0dip</item>
    </style>

    <style name="DialogRow">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center|left</item>
    </style>
    <style name="DialogRowText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center|right</item>
    </style>
    <style name="DialogText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
    </style>
    <style name="DialogTheme" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/sos</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:padding">6.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="DialogThemeMain">
        <item name="android:windowBackground">@drawable/sos</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="DialogThemeMainText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/transparentwhite</item>
    </style>
    <style name="DialogThemeMainTitle">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/transparentwhite</item>
    </style>
    <style name="DialogTitle">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="OptionText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center|left</item>
        <item name="android:paddingLeft">46.0dip</item>
    </style>
    <style name="SettingsTextBottom">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">right</item>
    </style>
    <style name="SettingsTopText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">right</item>
        <item name="android:padding">0.0dip</item>
    </style>
    <style name="SettingsTopTitle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/FadeRed</item>
        <item name="android:gravity">right</item>
        <item name="android:padding">0.0dip</item>
    </style>
    <style name="settingsTitleText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">right</item>
    </style>
    <style name="sliderMainText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:maxWidth">140.0dip</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="TestText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/FadeGreen</item>
    </style>
    <style name="ToastMessageText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/transparentwhite</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="VersionText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/transparentwhite</item>
    </style>

    <!-- Base application theme. -->
    <style name="MedicationAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryy</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkk</item>
        <item name="colorAccent">@color/colorAccentt</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="MedicationAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="MedicationAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="MedicationAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Label" parent="@style/TextAppearance.AppCompat.Medium">
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimaryy</item>
        <item name="android:background">@color/colorLabelBackground</item>
        <item name="android:padding">@dimen/_8sdp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MedicationColor">
        <item name="android:layout_width">@dimen/_35sdp</item>
        <item name="android:layout_height">@dimen/_35sdp</item>
        <item name="android:layout_marginTop">@dimen/_10sdp</item>
    </style>

    <style name="MedicationTypeIcon">
        <item name="android:layout_width">@dimen/_49sdp</item>
        <item name="android:layout_height">@dimen/_49sdp</item>
        <item name="android:layout_marginTop">@dimen/_10sdp</item>
    </style>

    <style name="TextMedium" parent="@style/TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@color/colorPrimaryy</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MedicationOverviewName" parent="@style/TextMedium">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimaryDarkk</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="TextSmall" parent="@style/TextAppearance.AppCompat.Small">
        <item name="android:textColor">@color/colorPrimaryy</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MedicationOverviewMinor" parent="@style/TextSmall">
        <item name="android:textColor">@color/colorPrimaryy</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

</resources>
